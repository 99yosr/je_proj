datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model InfosJets {
  id          Int      @id @default(autoincrement())
  titre       String
  description String
  logo        String
  service     String
  lien        String
  createdAt   DateTime @default(now())
}